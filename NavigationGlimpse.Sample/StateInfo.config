<StateInfo>
	<dialog key="Sample" initial="Page1" path="~/Page1.aspx">
		<state key="Page1" page="~/Page1.aspx" route="Page1">
			<transition key="Sort" to="Page1"/>
			<transition key="Next" to="Page2"/>
		</state>
		<state key="Page2" page="~/Page2.aspx" route="Page2">
			<transition key="Filter" to="Page2"/>
			<transition key="Sort" to="Page2"/>
			<transition key="Back" to="Page1"/>
		</state>
	</dialog>
	<dialog key="Sample2" initial="Page1" path="~/Page1.aspx">
		<state key="Page1" page="~/Page1.aspx">
			<transition key="Next" to="Page2"/>
		</state>
		<state key="Page2" page="~/Page2.aspx">
			<transition key="Next" to="Page3"/>
		</state>
		<state key="Page3" page="~/Page3.aspx" route="Page3">
			<transition key="Start" to="Page1"/>
		</state>
	</dialog>
</StateInfo>
